{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "license": "(MIT-0 OR Apache-2.0)",
  "scripts": {
    "allow-scripts": "yarn workspace root allow-scripts",
    "build": "GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "clean": "rimraf public",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "start": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@metamask/providers": "^18.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "styled-components": "5.3.3",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@metamask/eslint-config": "^13.0.0",
    "@metamask/eslint-config-browser": "^13.0.0",
    "@metamask/eslint-config-jest": "^13.0.0",
    "@metamask/eslint-config-nodejs": "^13.0.0",
    "@metamask/eslint-config-typescript": "^13.0.0",
    "@svgr/webpack": "^6.4.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-x": "^0.5.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsdoc": "^43.0.7",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "gatsby": "^5.13.3",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-styled-components": "^6.13.1",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "rimraf": "^3.0.2",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.6.0"
  }
}
